{
  "expo": {
    "name": "market-place",
    "slug": "market-place",
    "privacy": "public",
    "description": "A Market place to allow B2C operations.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/poster.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationWhenInUseUsageDescription": "text",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "text",
        "NSLocationAlwaysUsageDescription": "text"
      },
      "bundleIdentifier": "com.rsingh11.marketplace"
    },
    "android": { "icon": "./app/assets/poster.png"},
    "web": {
      "poster": "./assets/poster.png"
    },
    "extra": {
      "eas": {
        "projectId": "186f8e46-a475-4bd4-aafe-1685c6ced60c"
      }
    }
  }
}
